{
  "expo": {
    "name": "Tư Vấn Chuyển Đổi Số",
    "slug": "ChuyenDoiSoApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_an_giang.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash_2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tranquyensinh.ChuyenDoiSoApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/playstore-icon.png",
        "backgroundColor": "#406abe"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.tranquyensinh.ChuyenDoiSoApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ic_launcher-web.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Yêu cầu cấp quyền sử dụng thư viện Ảnh"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cbf87da9-5cc3-48be-b19a-8e32ff48475a"
      }
    }
  }
}